{
  "manifest_version": 3,
 
  "name": "StudocuHack",
  "version": "2.0.0",
  "description": "Chrome/Firefox Extension for Free Premium Studocu/Studeersnel",
  "icons": { 
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png" },
 
  "content_scripts": [{
    "js": ["check-ver.js", "remove-bar.js", "view-doc.js"],
    "css": ["style.css"],
    "matches": ["https://www.studocu.com/*", "https://www.studeersnel.nl/*"]
  }]
}